<% if notice %>
  <p class="notice" ><%= notice %></p>
<% end %>

<div class=header>
  <div class="header-title header-cells">Users</div>
  <div class="header-cells right"><%= link_new('user')%></div>
</div>

<table width="100%">
  <% @users.each do |user| %>
    <tr class="<%= cycle('list_line_odd', 'list_line_even') %>">
      <td><b><%= link_to(user.code, user) %></b></td>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= creator(user) %></td>
      <td><%= approver(user) %></td>
      <td><%= processor(user) %></td>
      <td><%= admin(user) %></td>
    </tr>
  <% end %>
</table>