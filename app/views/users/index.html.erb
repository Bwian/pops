<div class="admin_form">  
  <% if notice %>
    <p class="notice" ><%= notice %></p>
  <% end %>

  <table>
    <%= index_header('user',4)%>
    <% @users.each do |user| %>
      <tr class="<%= cycle('list_line_odd', 'list_line_even') %>">
        <td><b><%= link_to(user.code, user) %></b></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= admin(user) %></td>
      </tr>
    <% end %>
  </table>
</div>
